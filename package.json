    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@chatmcp/sdk": "^1.0.5",
    "@modelcontextprotocol/sdk": "0.6.0",
    "axios": "^1.4.0",
    "dotenv": "^17.2.0",
    "qs": "^6.14.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.11.24",
    "@types/qs": "^6.14.0",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.3.3"
  }
}
